<fieldset>
  <div class='row-fluid travel-item'>
    <div class="span1">
      <%= f.label :travel_from %>
    </div>
    <div class='span4'>
      <%= select_stations_for_travel_line(f, :travel_from) %>
    </div>
    <div class="span1">
      <%= f.label :travel_to %>
    </div>
    <div class='span5'>
      <%= select_stations_for_travel_line(f, :travel_to) %>
    </div>
  </div>
  <div class='row-fluid'>
    <div class='span6 form-horizontal'>
      <%= select_rise_time_with_interval(f) %>
    </div>
    <div class='span5'>
      <%= f.input :travel_days, as: :hidden, input_html: { value: day } %>
    </div>
  </div>
</fieldset>
